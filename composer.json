{
    "name": "daydiff/yii2-auth-chain",
    "description": "Yii2 extension implementing authorization chain",
    "type": "yii2-extension",
    "keywords": [
        "daydiff",
        "yii2",
        "extension",
        "authorization",
        "chain",
        "session"
    ],
    "homepage": "https://github.com/daydiff/yii2-auth-chain",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Tabakov",
            "email": "alex.daymon@gmail.com"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "yiisoft/yii2": "*"
    },
    "require-dev": {
        "codeception/codeception": "*",
        "codeception/specify": "*",
        "codeception/verify": "*",
        "yiisoft/yii2-codeception": "*"
    },
    "autoload": {
        "psr-4": {
            "Daydiff\\AuthChain\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Daydiff\\AuthChain\\Tests\\": "tests"
        }
    }
}
